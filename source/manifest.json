{
	"name": "__MSG_extensionName__",
	"version": "1.0.0",
	"description": "__MSG_extensionDescription__",
	"homepage_url": "https://sites-reviews.com",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"180": "icon180.png",
		"192": "icon192.png",
		"512": "icon512.png"
	},
	"permissions": [
		"storage",
		"tabs"
	],
	"background": {
		"persistent": false,
		"scripts": [
			"hot-reload.js",
			"browser-polyfill.min.js",
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": "icon180.png"
	},
	"default_locale": "en"
}
